{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "types": [
      "@dcloudio/types",
      "node"
    ],
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"]
  },
  "exclude": [
    "node_modules",
    "unpackage",
    "dist"
  ],
  "vueCompilerOptions": {
    "experimentalCompatMode": 2,
    "experimentalTemplateCompilerOptions": {
      "whitespace": "preserve"
    }
  }
}

